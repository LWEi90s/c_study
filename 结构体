int add(int x,int y)
{
	int z = 0;
	z = x + y;
	return z;


}


//栈区 堆区 静态区
int main()
{

	int a = 10;
	int b = 20;
	int ret = add(a, b);

	printf("%d\n", ret);

	return 0;

}
